CREATE TABLE `hiringportal`.`employees`(`id` int( 11 ) NOT NULL AUTO_INCREMENT ,`name` varchar( 30 ) NOT NULL,`skills` varchar( 50 ) NOT NULL,PRIMARY KEY ( `id` ));

