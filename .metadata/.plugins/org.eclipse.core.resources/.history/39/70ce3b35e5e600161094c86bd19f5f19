
public class MultipleCatch {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        try{
        	 int a[] = new int[7];
        	 a[4]=30/0;
        	 System.out.println("Last statement in try block");
        }catch(){
        	
        }catch(){
        	
        }catch(){
        	
        }
	}

}
