class MultiThread implements Runnable{
	public void run(){
		System.out.println("Thread stARTED name: "+Thread.currentThread().getName());
		try {
			Thread.sleep(4000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
